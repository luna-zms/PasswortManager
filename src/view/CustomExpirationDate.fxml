<fx:root prefHeight="26.0" prefWidth="325.0" type="javafx.scene.layout.GridPane" xmlns="http://javafx.com/javafx/8.0.999-ea" xmlns:fx="http://javafx.com/fxml/1">
   <columnConstraints>
      <ColumnConstraints hgrow="SOMETIMES" maxWidth="317.0" minWidth="10.0" prefWidth="161.0" />
      <ColumnConstraints hgrow="SOMETIMES" maxWidth="281.0" minWidth="10.0" prefWidth="146.0" />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
   </rowConstraints>
   <children>
      <HBox alignment="CENTER_LEFT" prefHeight="26.0" prefWidth="192.0">
         <children>
            <CheckBox fx:id="checkBoxExpirationDate" mnemonicParsing="false" />
            <DatePicker fx:id="datePickerExpirationDate" prefHeight="26.0" prefWidth="255.0" promptText="Ablaufdatum" />
         </children>
         <GridPane.margin>
            <Insets left="5.0" right="5.0" />
         </GridPane.margin></HBox>
      <HBox alignment="CENTER_RIGHT" prefHeight="26.0" prefWidth="96.0" GridPane.columnIndex="1">
         <children>
            <Label text="In Tagen:">
               <HBox.margin>
                  <Insets right="5.0" />
               </HBox.margin>
            </Label>
            <Spinner fx:id="daysUntilExpiration" editable="true" prefHeight="26.0" prefWidth="72.0" />
         </children>
         <GridPane.margin>
            <Insets left="5.0" right="5.0" />
         </GridPane.margin>
      </HBox>
   </children>
