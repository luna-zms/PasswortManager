<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.layout.HBox?>

<HBox xmlns:fx="http://javafx.com/fxml/1" fillHeight="false" maxHeight="-Infinity" maxWidth="-Infinity"
      minHeight="-Infinity"
      minWidth="-Infinity" prefHeight="26.0" prefWidth="301.0" style="-fx-background-color: white;"
      xmlns="http://javafx.com/javafx/11.0.1">
    <PasswordField fx:id="textField" maxWidth="1.7976931348623157E308" prefHeight="26.0"
                   style="-fx-background-color: transparent;" styleClass=".password-field" HBox.hgrow="ALWAYS"/>
    <Button fx:id="eyeButton" alignment="CENTER_RIGHT" mnemonicParsing="false" prefHeight="26.0"
            style="-fx-background-color: transparent;" styleClass=".password-button" text="E"/>
    <Button fx:id="checkButton" alignment="CENTER_RIGHT" mnemonicParsing="false" prefHeight="26.0"
            style="-fx-background-color: transparent;" styleClass=".password-button" text="C"/>

</HBox>
