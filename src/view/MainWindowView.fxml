                                    <rowConstraints>
                                        <RowConstraints maxHeight="54.0" minHeight="10.0" prefHeight="32.0"
                                                        vgrow="SOMETIMES" />
                                        <RowConstraints maxHeight="59.0" minHeight="10.0" prefHeight="32.0"
                                                        vgrow="SOMETIMES" />
                                        <RowConstraints maxHeight="100.0" minHeight="10.0" prefHeight="96.0"
                                                        vgrow="SOMETIMES" />
                                    </rowConstraints>
                                    <children>
                                        <Label text="Eintragsname:" GridPane.valignment="TOP">
                                            <GridPane.margin>
                                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
