                                                  GridPane.valignment="TOP">
                                            <GridPane.margin>
                                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                                            </GridPane.margin>
                                        </ListView>
                                    </children>
                                </GridPane>
                            </children>
                        </AnchorPane>
                    </children>
                </VBox>
            </children>
        </HBox>
    </center>
    <top>
        <MainWindowToolbarViewController />
    </top>
    <left>
        <TagTree fx:id="tagTree" />
    </left>
</fx:root>
